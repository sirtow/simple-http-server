{
  "name": "simple-http-server",
  "version": "0.1.8",
  "author": "Andrew Thorp <andrewpthorp@gmail.com>",
  "homepage": "https://github.com/andrewpthorp/simple-http-server",
  "preferGlobal": true,
  "description": "Simple HTTP Server, similar to python's SimpleHTTPServer",
  "keywords": [
